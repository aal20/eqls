{
  "name": "echoquery-workspace",
  "version": "1.0.0",
  "description": "Echo Query Language Server and Client",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "start:server": "npm run start -w @echoquery/server",
    "start:client": "npm run start -w @echoquery/client",
    "dev:server": "npm run dev -w @echoquery/server",
    "dev:client": "npm run dev -w @echoquery/client"
  }
}
